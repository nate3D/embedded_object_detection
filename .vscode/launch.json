{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug object_detect",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/object_detect",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "BuildAndDebugObjectDetect",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "Debug aws_client",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/aws_client",
            "args": ["--debug"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "BuildAndDebugAWSClient",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}
